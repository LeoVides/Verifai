<% @results.each do |result| %>
  <div class="card-saved">
    <h1><%= result.title %></h1>
    <p>Fact score: <%= result.fact_score %> Confidence: <%= result.fact_score_confidence%></p>
    <p>Political bias: <%= result.political_bias %>Confidence: <%= result.political_bias_confidence%></p>
  </div>
<% end %>
